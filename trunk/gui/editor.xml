<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<theme name="areasbox" ref="scrollpane">
        <param name="minWidth"><int>350</int></param>
        <param name="minHeight"><int>300</int></param>
    </theme>
	
	<theme name="editorlayout" ref="dialoglayout" allowWildcard="true">
        <theme name="label" ref="labelblack"/>
        <theme name="button" ref="button"/>
        <theme name="editfield" ref="editfield"/>
        <theme name="editfieldlarge" ref="editfield">
            <param name="minWidth"><int>275</int></param>
        </theme>
        <theme name="combobox" ref="combobox"/>
        <theme name="valueadjuster" ref="valueadjuster"/>
        <theme name="togglebutton" ref="radiobutton">
            <param name="maxHeight"><int>20</int></param>
        </theme>
        <theme name="listbox" ref="listbox"/>
        <theme name="listboxsmall" ref="listbox">
            <param name="maxWidth"><int>100</int></param>
            <param name="minWidth"><int>100</int></param>
        </theme>
        <theme name="listboxlarge" ref="listbox">
            <param name="minWidth"><int>300</int></param>
        </theme>
        <theme name="listboxnoexpand" ref="listbox">
            <param name="maxWidth"><int>250</int></param>
            <param name="minWidth"><int>200</int></param>
        </theme>
        <theme name="scrollpane" ref="scrollpane"/>
        <theme name="lootscrollpane" ref="scrollpane">
            <param name="minWidth"><int>255</int></param>
        </theme>
        <theme name="smallcombobox" ref="combobox">
            <param name="minWidth"><int>150</int></param>
        </theme>
        <theme name="mediumcombobox" ref="combobox">
            <param name="minWidth"><int>250</int></param>
        </theme>
        <theme name="largecombobox" ref="combobox">
            <param name="minWidth"><int>350</int></param>
        </theme>
        <theme name="smalladjuster" ref="valueadjuster">
            <param name="width"><int>30</int></param>
        </theme>

        <theme name="tabbedpane" ref="tabbedpane"/>
    </theme>
	
	<theme name="filepopup" ref="editorlayout">
		<param name=""><image>iconbutton.*</image></param>
        <param name="border"><border>20,20,20,20</border></param>
        <param name="defaultGap"><dimension>5,5</dimension></param>
		
		<theme name="button" ref="button">
			<param name="minWidth"><int>50</int></param>
			<param name="minHeight"><int>30</int></param>
		</theme>
	</theme>
	
	<theme name="gamepopup" ref="editorlayout" allowWildcard="true">
        <param name=""><image>iconbutton.*</image></param>
        <param name="border"><border>background.border</border></param>
        <param name="defaultGap"><dimension>5,0</dimension></param>
    </theme>

    <theme name="itemlistentrypane" ref="gamepopup" allowWildcard="true">
        <theme name="valueadjuster" ref="valueadjuster">
            <param name="maxWidth"><int>85</int></param>
        </theme>
    </theme>
	
    <theme name="locationeditorlayout" ref="dialoglayout" allowWildcard="true">
        <theme name="listbox" ref="listbox">
            <param name="maxWidth"><int>150</int></param>
            <param name="maxHeight"><int>250</int></param>
        </theme>
        <theme name="combobox" ref="combobox">
            <param name="minWidth"><int>210</int></param>
        </theme>
        <theme name="valueadjuster" ref="valueadjuster"/>
        <theme name="label" ref="labelblack"/>
        <theme name="editfield" ref="editfield">
            <param name="maxWidth"><int>150</int></param>
        </theme>
        <theme name="button" ref="button"/>
        <theme name="radiobutton" ref="radiobutton"/>
        <theme name="scrollpane" ref="scrollpane">
            <param name="minWidth"><int>150</int></param>
            <theme name="content" ref="dialoglayout"/>
			<theme name="worldmapviewer" ref="-defaults">
				<theme name="locationviewer" ref="-defaults"/>
				
				<theme name="locationhover" ref="-defaults">
					<param name="border"><border>5</border></param>
					<param name="background"><image>-borderH</image></param>
					<theme name="label" ref="labelbigblack"/>
					<theme name="button" ref="button">
						<param name="minWidth"><int>70</int></param>
						<param name="minHeight"><int>25</int></param>
					</theme>
				</theme>
			</theme>
        </theme>
    </theme>

    <theme name="encountereditorlayout" ref="dialoglayout" allowWildcard="true">
        <theme name="scrollpane" ref="scrollpane"/>
        <theme name="listbox" ref="listbox">
            <param name="maxWidth"><int>250</int></param>
            <param name="minHeight"><int>200</int></param>
        </theme>
    </theme>

    <theme name="scripteditorlayout" ref="dialoglayout" allowWildcard="true">
		<param name="minWidth"><int>400</int></param>
		<param name="minHeight"><int>300</int></param>
		
		<theme name="menubar" ref="menumanager.menubar"/>
		
        <theme name="scrollpane" ref="scrollpane">
            <theme name="editfield" ref="editfield">
				<param name="background"><image>none</image></param>
				<theme name="renderer" ref="-defaults">
					<param name="textAlignment"><enum type="alignment">topleft</enum></param>
					<param name="font"><font>code</font></param>
				</theme>
			</theme>
            <theme name="textarea" ref="-defaults">
                <param name="fonts"><map>
                     <param name="default"><font>black</font></param>
                     <param name="frame"><font>normal</font></param>
                     <param name="link"><font>link</font></param>
                     <param name="code"><font>code</font></param>
                     <param name="codeComment"><font>codeComment</font></param>
                     <param name="codeCommentTag"><font>codeCommentTag</font></param>
                     <param name="codeString"><font>codeString</font></param>
                     <param name="codeKeyword"><font>codeKeyword</font></param>
                 </map></param>
                 <param name="images"><map>
                     <param name="ul-bullet"><image>textarea.ul-bullet</image></param>
                 </map></param>
                 <param name="mouseCursor.link"><cursor>cursor.finger</cursor></param>
                 <theme name="" ref="*"/>
             </theme>
        </theme>
		<theme name="listbox" ref="listbox">
            <param name="maxWidth"><int>250</int></param>
        </theme>
    </theme>
</themes>