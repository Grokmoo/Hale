		<select name="endturnbutton.background" border="3">
			<alias ref="endturnicon" if="disabled" tint="#8fff"/>
			<animation if="notification + armed" timeSource="notification">
				<frame ref="endturniconpressed" tint="#f88" duration="500"/>
				<frame ref="endturniconpressed" tint="#fff" duration="500"/>
			</animation>
			<animation if="notification + hover" timeSource="notification">
				<frame ref="endturniconhover" tint="#f88" duration="500"/>
				<frame ref="endturniconhover" tint="#fff" duration="500"/>
			</animation>
			<animation if="notification" timeSource="notification">
				<frame ref="endturnicon" tint="#f88" duration="500"/>
				<frame ref="endturnicon" tint="#fff" duration="500"/>
			</animation>
            <alias ref="endturniconpressed" if="armed"/>
			<alias ref="endturniconhover" if="hover"/>
            <alias ref="endturnicon"/>
		</select>
		
