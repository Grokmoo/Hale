        </select>
		
		<area name="panebackground-gothic" border="7" xywh="0,192,72,72" splitx="L24,R24" splity="T24,B24"/>
		
		<area name="smallbackground-gothic" border="3" xywh="0,264,27,27" splitx="L9,R9" splity="T9,B9"/>
