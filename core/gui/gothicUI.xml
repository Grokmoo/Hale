<themes>
    <images file="gothicUI.png" format="RGBA">
	    <area name="windowbackground-gothic" border="93" xywh="0,0,192,192" splitx="L64,R64" splity="T64,B64"/>
		
		<area name="windowclosebutton-normal" xywh="192,0,17,17"/>
		<area name="windowclosebutton-hover" xywh="192,17,17,17"/>
		<area name="windowclosebutton-armed" xywh="192,34,17,17"/>
		
		<select name="windowclosebutton.background" border="2,2,1,1">
